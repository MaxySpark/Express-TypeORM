{
   "name": "tsc-express-typeorm",
   "version": "1.0.0",
   "description": "Express App in Typescript with TypeORM",
   "main": "dist/index.js",
   "scripts": {
      "test": "jest",
      "start": "node .",
      "start:dev": "nodemon --watch src -e ts,pug,css --exec ts-node src/index.ts",
      "clean": "rimraf dist/*",
      "copy-assets": "ts-node tools/copyAssets",
      "tsc": "tsc",
      "build": "npm-run-all clean tsc copy-assets",
      "dev:start": "npm-run-all build start",
      "dev": "nodemon --watch src -e ts,pug,css --exec npm run dev:start",
      "migration:create": "ts-node ./node_modules/typeorm/cli migration:create",
      "migration:generate": "ts-node ./node_modules/typeorm/cli migration:generate",
      "migration:run": "ts-node ./node_modules/typeorm/cli migration:run",
      "migration:revert": "ts-node ./node_modules/typeorm/cli migration:revert"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/MaxySpark/Express-TypeORM.git"
   },
   "keywords": [
      "tsc",
      "nodejs",
      "express",
      "typeorm"
   ],
   "author": "MaxySpark <me@maxyspark.me>",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/MaxySpark/Express-TypeORM/issues"
   },
   "homepage": "https://github.com/MaxySpark/Express-TypeORM#readme",
   "devDependencies": {
      "@types/app-root-path": "^1.2.4",
      "@types/bcrypt": "^3.0.0",
      "@types/body-parser": "^1.17.0",
      "@types/cors": "^2.8.5",
      "@types/email-templates": "^3.5.1",
      "@types/express": "^4.16.1",
      "@types/jest": "^24.0.18",
      "@types/jsonwebtoken": "^8.3.2",
      "@types/lodash": "^4.14.135",
      "@types/morgan": "^1.7.35",
      "@types/multer": "^1.3.7",
      "@types/multer-s3": "^2.7.6",
      "@types/node": "^8.0.29",
      "@types/nodemailer": "^4.6.7",
      "@types/request": "^2.48.1",
      "@types/request-promise": "^4.1.43",
      "@types/shelljs": "^0.8.5",
      "jest": "^24.9.0",
      "nodemon": "^1.19.1",
      "npm-run-all": "^4.1.5",
      "rimraf": "^2.6.3",
      "shelljs": "^0.8.3",
      "ts-jest": "^24.0.2",
      "ts-node": "3.3.0",
      "typescript": "3.4"
   },
   "dependencies": {
      "app-root-path": "^2.2.1",
      "aws-sdk": "^2.488.0",
      "bcrypt": "^3.0.5",
      "body-parser": "^1.18.3",
      "class-transformer": "^0.2.0",
      "class-validator": "^0.14.0",
      "cors": "^2.8.5",
      "dotenv": "^7.0.0",
      "email-templates": "^5.1.0",
      "express": "^4.16.4",
      "generate-password": "^1.4.1",
      "google-auth-library": "^3.1.2",
      "googleapis": "^39.2.0",
      "jsonwebtoken": "^8.5.1",
      "lodash": "^4.17.15",
      "moment": "^2.24.0",
      "morgan": "^1.9.1",
      "multer": "^1.4.1",
      "multer-s3": "^2.9.0",
      "mysql": "^2.14.1",
      "nodemailer": "^6.0.0",
      "pug": "^2.0.3",
      "reflect-metadata": "^0.1.10",
      "request": "^2.88.0",
      "request-promise": "^4.2.4",
      "typeorm": "0.2.15",
      "winston": "^3.2.1"
   }
}
